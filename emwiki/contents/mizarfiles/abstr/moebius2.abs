theorem :: MOEBIUS2:8
  primenumber 0 = 2;

theorem :: MOEBIUS2:9
  SetPrimenumber 3 = {2};

theorem :: MOEBIUS2:10
  primenumber 1 = 3;

theorem :: MOEBIUS2:11
  SetPrimenumber 5 = {2, 3};

theorem :: MOEBIUS2:12
  primenumber 2 = 5;

theorem :: MOEBIUS2:13
  SetPrimenumber 6 = {2, 3, 5};

theorem :: MOEBIUS2:14
  SetPrimenumber 7 = {2, 3, 5};

theorem :: MOEBIUS2:15
  primenumber 3 = 7;

theorem :: MOEBIUS2:16
