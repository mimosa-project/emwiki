definition
  let X,x,y;
  pred x <<>> y means
:: ABSRED_0:def 20
  ex z st x <=*= z & z =*=> y;
  symmetry;
  reflexivity;
  pred x >><< y means
:: ABSRED_0:def 21
  ex z st x =*=> z & z <=*= y;
  symmetry;
  reflexivity;
  pred x <<01>> y means
:: ABSRED_0:def 22
  ex z st x <=01= z & z =01=> y;
  symmetry;
  reflexivity;
  pred x >>01<< y means
:: ABSRED_0:def 23
  ex z st x =01=> z & z <=01= y;
  symmetry;
  reflexivity;
end;

