 ; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> <font color="Maroon" title="c7">b</font> <a href="afinsq_2.html#K6" title="AFINSQ_2:func.6">"**"</a> <span class="p1">(<span class="default"><font color="Maroon" title="c6">F</font> <a href="ordinal4.html#K1" title="ORDINAL4:func.1">^</a> <span class="p2">(<span class="default"><a href="afinsq_1.html#K4" title="AFINSQ_1:func.4">&lt;%&gt;</a> <font color="Maroon" title="c2">D</font></span>)</span></span>)</span> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon" title="c7">b</font> <a href="binop_1.html#K4" title="BINOP_1:func.4">.</a> (<span class="p1">(<span class="default"><font color="Maroon" title="c7">b</font> <a href="afinsq_2.html#K6" title="AFINSQ_2:func.6">"**"</a> <font color="Maroon" title="c6">F</font></span>)</span>,<span class="p1">(<span class="default"><font color="Maroon" title="c7">b</font> <a href="afinsq_2.html#K6" title="AFINSQ_2:func.6">"**"</a> <span class="p2">(<span class="default"><a href="afinsq_1.html#K4" title="AFINSQ_1:func.4">&lt;%&gt;</a> <font color="Maroon" title="c2">D</font></span>)</span></span>)</span>)</span><br/>

<span class="kw">thus </span><font color="Maroon" title="c7">b</font> <a href="afinsq_2.html#K6" title="AFINSQ_2:func.6">"**"</a> <span class="p1">(<span class="default"><font color="Maroon" title="c6">F</font> <a href="ordinal4.html#K1" title="ORDINAL4:func.1">^</a> <span class="p2">(<span class="default"><a href="afinsq_1.html#K4" title="AFINSQ_1:func.4">&lt;%&gt;</a> <font color="Maroon" title="c2">D</font></span>)</span></span>)</span> = 
