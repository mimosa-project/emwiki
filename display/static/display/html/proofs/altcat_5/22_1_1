 ; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide">  ex <font color="Olive" title="b1">f</font> being   <a href="altcat_1.html#NM2" title="ALTCAT_1:NM.2">Morphism</a> of <font color="Maroon" title="c10">Y</font>,<font color="Maroon" title="c8">B</font> st <br/>( <font color="Olive" title="b1">f</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a> <span class="p1"><a href="altcat_1.html#K1" title="ALTCAT_1:func.1">&lt;^</a><span class="default"><font color="Maroon" title="c10">Y</font>,<font color="Maroon" title="c8">B</font></span><a href="altcat_1.html#K1" title="ALTCAT_1:func.1">^&gt;</a></span> &amp; (  for <font color="Olive" title="b2">i</font> being    <a href="subset_1.html#M1" title="SUBSET_1:mode.1">Element</a> of <font color="Maroon" title="c6">I</font> holds  <font color="Maroon" title="c11">F</font> <a href="altcat_5.html#K1" title="ALTCAT_5:func.1">.</a> <font color="Olive" title="b2">i</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <span class="p1">(<span class="default"><font color="Maroon" title="c9">P</font> <a href="altcat_5.html#K1" title="ALTCAT_5:func.1">.</a> <font color="Olive" title="b2">i</font></span>)</span> <a href="altcat_1.html#K5" title="ALTCAT_1:func.5">*</a> <font color="Olive" title="b1">f</font> ) &amp; (  for <font color="Olive" title="b2">f1</font> being   <a href="altcat_1.html#NM2" title="ALTCAT_1:NM.2">Morphism</a> of <font color="Maroon" title="c10">Y</font>,<font color="Maroon" title="c8">B</font>  st (  for <font color="Olive" title="b3">i</font> being    <a href="subset_1.html#M1" title="SUBSET_1:mode.1">Element</a> of <font color="Maroon" title="c6">I</font> holds  <font color="Maroon" title="c11">F</font> <a href="altcat_5.html#K1" title="ALTCAT_5:func.1">.</a> <font color="Olive" title="b3">i</font> <a href="hidden.html#R1" title="HID