<span class="kw">hence </span><a NAME="E2:20_19_1_1_1"/>
<span class="p1">(<span class="default"><font color="Maroon" title="c1">N1</font> <a href="matrix_3.html#K4" title="MATRIX_3:func.4">*</a> <span class="p2">(<span class="default"><span class="p3"><a href="laplace.html#K10" title="LAPLACE:func.10">&lt;*</a><span class="default"><font color="Maroon" title="c14">u1f</font></span><a href="laplace.html#K10" title="LAPLACE:func.10">*&gt;</a></span> <a href="matrix_0.html#K4" title="MATRIX_0:func.4">@</a></span>)</span></span>)</span> <a href="matrix_0.html#K3" title="MATRIX_0:func.3">*</a> (1,1) <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <span class="p1">(<span class="default"><font color="Maroon" title="c16">a</font> <a href="xcmplx_0.html#K3" title="XCMPLX_0:func.3">*</a> <font color="Maroon" title="c17">b</font></span>)</span> <a href="xcmplx_0.html#K3" title="XCMPLX_0:func.3">*</a> <span class="p1">(<span class="default"><span class="p2">(<span class="default"><span class="p3">(<span class="default"><font color="Maroon" title="c1">N1</font> <a href="matrix_3.html#K4" title="MATRIX_3:func.4">*</a> <font color="Maroon" title="c2">N2</font></span>)</span> <a href="matrix_3.html#K4" title="MATRIX_3:func.4">*</a> <span class="p3">(<span class="default"><span class="p4"><a href="laplace.html#K10" title="LAPLACE:func.10">&lt;*</a><span class="default"><font color="Maroon" title="c6">u12f</font></span><a href="laplace.html#K10" title="LAPLACE:func.10">*&gt;</a></span> <a href="matrix_0.html#K4" title="MATRIX_0:func.4">@</a></span>)</span></span>)</span> <a href="matrix_0.html#K3" title="MATRIX_0:func.3">*</a> (1,1)</span>)</span>
