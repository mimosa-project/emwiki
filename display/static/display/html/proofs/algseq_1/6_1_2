<span class="kw">let </span><font color="Maroon" title="c3">k</font>, <font color="Maroon" title="c4">l</font> be    <a href="subset_1.html#M1" title="SUBSET_1:mode.1">Element</a> of  <a href="numbers.html#NK1" title="NUMBERS:NK.1">NAT</a> ; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> ( <font color="Maroon" title="c3">k</font> <a href="algseq_1.html#R1" title="ALGSEQ_1:pred.1">is_at_least_length_of</a> <font color="Maroon" title="c2">p</font> &amp; (  for <font color="Olive" title="b1">m</font> being   <a href="nat_1.html#NM1" title="NAT_1:NM.1">Nat</a>  st <font color="Olive" title="b1">m</font> <a href="algseq_1.html#R1" title="ALGSEQ_1:pred.1">is_at_least_length_of</a> <font color="Maroon" title="c2">p</font> holds <br/><font color="Maroon" title="c3">k</font> <a href="xxreal_0.html#R1" title="XXREAL_0:pred.1">&lt;=</a> <font color="Olive" title="b1">m</font> ) &amp; <font color="Maroon" title="c4">l</font> <a href="algseq_1.html#R1" title="ALGSEQ_1:pred.1">is_at_least_length_of</a> <font color="Maroon" title="c2">p</font> &amp; (  for <font color="Olive" title="b1">m</font> being   <a href="nat_1.html#NM1" title="NAT_1:NM.1">Nat</a>  st <font color="Olive" title="b1">m</font> <a href="algseq_1.html#R1" title="ALGSEQ_1:pred.1">is_at_least_length_of</a> <font color="Maroon" title="c2">p</font> holds <br/><font color="Maroon" title="c4">l</font> <a href="xxreal_0.html#R1" title="XXREAL_0:pred.1">&lt;=</a> <font color="Olive" title="b1">m</font> ) implies <font color="Maroon" title="c3">k</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon" title="c4">l</font> )</span><br/>

<span class="kw">assume </span><a NAME="E1:6_1_2"/>
( <font color="Maroon" title="c3">k</font> <a href="algseq_1.html#R1" title="ALGSEQ_1:pred.1">is_at_least_length_of</a> <font color="Maroon" title="c2">p</font> &amp; (  for <font color="Olive" title="b1">m</font> being   <a href="nat_1.html#NM1" title="NAT_1:NM.1">Nat</a>  st <font color="Olive" title="b1">m</font> <a href="algseq_1.html#R1" title="ALGSEQ_1:pred.1">is_at_least_length_of</a> <font color="Maroon" title="c2">p</font> holds <br/><font color="Maroon" title="c3">k</font> <a href="xxreal_0.html#R1" title="XXREAL_0:pred.1">&lt;=</a> <font color="Olive" title="b1">m</font> ) &amp; <font color="Maroon" title="c4">l</font> <a href="algseq_1.html#R1" title="ALGSEQ_1:pred.1">is_at_least_length_of</a> <font color="Maroon" title="c2">p</font> &amp; (  for <font color="Olive" title="b1">m</font> being   <a href="nat_1.html#NM1" title="NAT_1:NM.1">Nat</a>  st <font color="Olive" title="b1">m</font> <a href="algseq_1.html#R1" title="ALGSEQ_1:pred.1">is_at_least_length_of</a> <font color="Maroon" title="c2">p</font> holds <br/><font color="Maroon" title="c4">l</font> <a href="xxreal_0.html#R1" title="XXREAL_0:pred.1">&lt;=</a> <font color="Olive" title="b1">m</font> ) )
 ; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> <font color="Maroon" title="c3">k</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon" title="c4">l</font></span><br/>

<a NAME="E2:6_1_2"/><span class="kw">then </span>
