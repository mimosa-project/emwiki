<span class="kw">take </span>
<font color="Maroon" title="c11">N</font>
; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> ( <font color="Maroon" title="c11">N</font> is  <a href="matrix_6.html#V1" title="MATRIX_6:attr.1">invertible</a>  &amp; <font color="Maroon" title="c11">N</font> <a href="laplace.html#K11" title="LAPLACE:func.11">*</a> <font color="Maroon" title="c4">pf</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="anproj_8.html#K1" title="ANPROJ_8:func.1">F2M</a> <a href="euclid_8.html#K2" title="EUCLID_8:func.2">&lt;e1&gt;</a> &amp; <font color="Maroon" title="c11">N</font> <a href="laplace.html#K11" title="LAPLACE:func.11">*</a> <font color="Maroon" title="c5">qf</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="anproj_8.html#K1" title="ANPROJ_8:func.1">F2M</a> <a href="euclid_8.html#K3" title="EUCLID_8:func.3">&lt;e2&gt;</a> &amp; <font color="Maroon" title="c11">N</font> <a href="laplace.html#K11" title="LAPLACE:func.11">*</a> <font color="Maroon" title="c6">rf</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="anproj_8.html#K1" title="ANPROJ_8:func.1">F2M</a> <a href="euclid_8.html#K4" title="EUCLID_8:func.4">&lt;e3&gt;</a> )</span><br/>

<span class="kw">thus </span><a NAME="E16:95"/>
<font color="Maroon" title="c11">N</font> is  <a href="matrix_6.html#V1" title="MATRIX_6:attr.1">invertible</a> 
 <span class="kw">by</span> <span class="lab"><a class="txt" href="#E15:95"><span class="lab"><font color="Green" title="E87">A9</font></span></a>, <a class="ref" href="matrix_6.html#D3" onmouseover="rs('matrix_6/D3')" onmouseout="rh()">MATRIX_6:def 3</a></span>; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> ( <font color="Maroon" title="c11">N</font> <a href="laplace.html#K11" title="LAPLACE:func.11">*</a> <font color="Maroon" title="c4">pf</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="anproj_8.html#K1" title="ANPROJ_8:func.1">F2M</a> <a href="euclid_8.html#K2" title="EUCLID_8:func.2">&lt;e1&gt;</a> &amp; <font color="Maroon" title="c11">N</font> <a href="laplace.html#K11" title="LAPLACE:func.11">*</a> <font color="Maroon" title="c5">qf</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="anproj_8.html#K1" title="ANPROJ_8:func.1">F2M</a> <a href="euclid_8.html#K3" title="EUCLID_8:func.3">&lt;e2&gt;</a> &amp; <font color="Maroon" title="c11">N</font> <a href="laplace.html#K11" title="LAPLACE:func.11">*</a> <font color="Maroon" title="c6">rf</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="anproj_8.html#K1" title="ANPROJ_8:func.1">F2M</a> <a href="euclid_8.html#K4" title="EUCLID_8:func.4">&lt;e3&gt;</a> )</span><br/>

