<font color="Maroon" title="c1">X</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <span class="p1"> { <span class="default"> <span class="p2">(<span class="default"><font color="Olive" title="b1">w</font> <a href="xfamily.html#K1" title="XFAMILY:func.1">`1</a></span>)</span> where <font color="Olive" title="b1">w</font> is   <a href="struct_0.html#NM1" title="STRUCT_0:NM.1">Element</a> of <span class="p1">(<span class="default"><a href="algstr_4.html#K14" title="ALGSTR_4:func.14">free_magma</a> <font color="Maroon" title="c1">X</font></span>)</span> :  <a href="algstr_4.html#K15" title="ALGSTR_4:func.15">length</a> <font color="Olive" title="b1">w</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> 1 </span> } </span> 
