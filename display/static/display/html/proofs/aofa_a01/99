<span class="kw">let </span><font color="Maroon" title="c20">s0</font> be    <a href="subset_1.html#M2" title="SUBSET_1:mode.2">Element</a> of <font color="Maroon" title="c4">C</font> <a href="aofa_a00.html#K12" title="AOFA_A00:func.12">-States</a>  the <a href="aofa_a00.html#U4" title="AOFA_A00:sel.4">generators</a> of <font color="Maroon" title="c5">G</font>; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> ( <font color="Maroon" title="c12">f</font> <a href="aofa_a01.html#K3" title="AOFA_A01:func.3">.</a> (<font color="Maroon" title="c20">s0</font>,<span class="p1">(<span class="default"><font color="Maroon" title="c10">b</font> <a href="aofa_a01.html#K6" title="AOFA_A01:func.6">gt</a> (<span class="p2">(<span class="default"><a href="aofa_a01.html#K4" title="AOFA_A01:func.4">@</a> <font color="Maroon" title="c9">m</font></span>)</span>,<span class="p2">(<span class="default"><a href="aofa_a00.html#K26" title="AOFA_A00:func.26">\0</a> (<font color="Maroon" title="c3">T</font>,<font color="Maroon" title="c6">I</font>)</span>)</span>,<font color="Maroon" title="c11">A</font>)</span>)</span>) <a href="tarski.html#R2" title="TARSKI:pred.2">in</a> <span class="p1"> { <span class="default"> <font color="Olive" title="b1">s</font> where <font color="Olive" title="b1">s</font> is    <a href="subset_1.html#M2" title="SUBSET_1:mode.2">Element</a> of <font color="Maroon" title="c4">C</font> <a href="aofa_a00.html#K12" title="AOFA_A00:func.12">-States</a>  the <a href="aofa_a00.html#U4" title="AOFA_A00:sel.4">generators</a> of <font color="Maroon" title="c5">G</font> : <span class="p1">(<span class="default"><font color="Olive" title="b1">s</font> <a href="msafree4.html#K1" title="MSAFREE4:func.1">.</a> <font color="Maroon" title="c6">I</font></span>)</span> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</