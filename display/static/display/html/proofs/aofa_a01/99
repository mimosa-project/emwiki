<span class="kw">let </span><font color="Maroon" title="c20">s</font> be    <a href="subset_1.html#M2" title="SUBSET_1:mode.2">Element</a> of <font color="Maroon" title="c4">C</font> <a href="aofa_a00.html#K12" title="AOFA_A00:func.12">-States</a>  the <a href="aofa_a00.html#U4" title="AOFA_A00:sel.4">generators</a> of <font color="Maroon" title="c5">G</font>; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> ( <font color="Maroon">S<sub>1</sub></font>[<font color="Maroon" title="c20">s</font>] implies ( ( <font color="Maroon">S<sub>1</sub></font>[<font color="Maroon" title="c12">f</font> <a href="aofa_a01.html#K3" title="AOFA_A01:func.3">.</a> (<font color="Maroon" title="c20">s</font>,<span class="p1">(<span class="default"><span class="p2">(<span class="default"><span class="p3">(<span class="default"><span class="p4">(<span class="default"><a href="aofa_000.html#K15" title="AOFA_000:func.15">if-then</a> (<span class="p5">(<span class="default"><span class="p0">(<span class="default"><a href="aofa_a01.html#K4" title="AOFA_A01:func.4">@</a> <font color="Maroon" title="c9">m</font></span>)</span> <a href="aofa_a01.html#K8" title="AOFA_A01:func.8">is_odd</a> (<font color="Maroon" title="c10">b</font>,<font color="Maroon" title="c11">A</font>)</span>)</span>,<span class="p5">(<span class="default"><font color="Maroon" title="c8">y</font> <a href="aofa_a00.html#K11" title="AOFA_A00:func.11">:=</a> (<span class="p0">(<span class="default"><span class="p1">(<span class="default"><a href="aofa_a01.html#K4" title="AOFA_A01:func.4">@</a> <font color="Maroon" title="c8">y</font></span>)</span> <a href="aofa_a00.html#K30" title="AOFA_A00:func.30">*</a> <span class="p1">(<span class="default"><a href="aofa_a01.html#K4" title="AOFA_A01:func.4">@</a> <font color="Maroon" title="c7">x</font></span>)</span></span>)<