<span class="kw">let </span><font color="Maroon" title="c15">J</font> be   <a href="aofa_000.html#NM2" title="AOFA_000:NM.2">Algorithm</a> of <font color="Maroon" title="c13">A</font>; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> ( (  for <font color="Olive" title="b1">s</font> being    <a href="subset_1.html#M2" title="SUBSET_1:mode.2">Element</a> of <font color="Maroon" title="c4">C</font> <a href="aofa_a00.html#K12" title="AOFA_A00:func.12">-States</a>  the <a href="aofa_a00.html#U4" title="AOFA_A00:sel.4">generators</a> of <font color="Maroon" title="c5">G</font> holds <br/> ( <span class="p1">(<span class="default"><span class="p2">(<span class="default"><font color="Maroon" title="c14">f</font> <a href="aofa_a01.html#K3" title="AOFA_A01:func.3">.</a> (<font color="Olive" title="b1">s</font>,<font color="Maroon" title="c15">J</font>)</span>)</span> <a href="msafree4.html#K1" title="MSAFREE4:func.1">.</a> <span class="p2">(<span class="default"><a href="aofa_a00.html#K36" title="AOFA_A00:func.36">the_array_sort_of</a> <font color="Maroon" title="c1">S</font></span>)</span></span>)</span> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <font color="Maroon" title="c8">M</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <span class="p1">(<span class="default"><font color="Olive" title="b1">s</font> <a href="msafree4.html#K1" title="MSAFREE4:func.1">.</a> <span class="p2">(<span class="default"><a href="aofa_a00.html#K36" title="AOFA_A00:func.36">the_array_sort_of</a> <font color="Maroon" title="c1">S</font></span>)</span></span>)</span> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <font color="Maroon" title="c8">M</font> &amp; (  for <font color="Olive" title="b2">D</font> being   <a href="exchsort.html#NM3" title="EXCHSORT:NM.3">array</a> of <a href="aofa_a01.html#K12"