( <span class="p1"><a href="complex1.html#K9" title="COMPLEX1:func.9">|.</a><span class="default"><span class="p2">(<span class="default"><font color="Maroon" title="c1">x</font> <a href="xcmplx_0.html#K2" title="XCMPLX_0:func.2">+</a> <font color="Maroon" title="c2">y</font></span>)</span></span><a href="complex1.html#K9" title="COMPLEX1:func.9">.|</a></span> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <span class="p1"><a href="complex1.html#K9" title="COMPLEX1:func.9">|.</a><span class="default"><font color="Maroon" title="c1">x</font></span><a href="complex1.html#K9" title="COMPLEX1:func.9">.|</a></span> <a href="xcmplx_0.html#K2" title="XCMPLX_0:func.2">+</a> <span class="p1"><a href="complex1.html#K9" title="COMPLEX1:func.9">|.</a><span class="default"><font color="Maroon" title="c2">y</font></span><a href="complex1.html#K9" title="COMPLEX1:func.9">.|</a></span> &amp;  <a href="numbers.html#K5" title="NUMBERS:func.5">0</a>  <a href="xxreal_0.html#NR4" title="XXREAL_0:NR.4">&gt;</a> <font color="Maroon" title="c1">x</font> <a href="xcmplx_0.html#K3" title="XCMPLX_0:func.3">*</a> <font color="Maroon" title="c2">y</font> )
 ; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> contradiction</span><br/>

<span class="kw">hence </span><a NAME="E7:10"/>
contradiction
 <span class="kw">by</span> <span class="lab"><a class="txt" href="#E1:10"><span class="lab"><font color="Green" title="E2">A1</font></span></a>, <a class="txt" href="#E2:10"><span class="lab"><font color="Green" title="E3">A3</font></span></a>, <a class="txt" href="#E5:10"><span class="lab"><font color="Green" title="E6">A18</font></span></a>, <a class="txt" href="#E3:10"><span class="lab"><font color="Green" title="E4">A8</font></span></a>, <a class="txt" href="#E4:10"><span class="lab"><font color="Green" title="E5">A13</font></span></a></span>; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> verum</span><br/>


