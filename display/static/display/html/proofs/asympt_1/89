( <font color="Maroon" title="c13">n</font> <a href="xxreal_0.html#NR4" title="XXREAL_0:NR.4">&gt;</a>  <a href="numbers.html#K5" title="NUMBERS:func.5">0</a>  implies <font color="Maroon" title="c12">h</font> <a href="nat_1.html#K8" title="NAT_1:func.8">.</a> <font color="Maroon" title="c13">n</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="power.html#K3" title="POWER:func.3">log</a> (2,<font color="Maroon" title="c6">n</font>) )
