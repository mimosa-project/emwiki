<span class="kw">thus </span><a NAME="E4:43_1_1"/>
 for <font color="Olive" title="b1">Key</font> being    <a href="finseq_2.html#M2" title="FINSEQ_2:mode.2">Element</a> of <font color="Maroon" title="c2">m</font> <a href="finseq_2.html#K4" title="FINSEQ_2:func.4">-tuples_on</a> <span class="p1">(<span class="default">4 <a href="finseq_2.html#K4" title="FINSEQ_2:func.4">-tuples_on</a> <span class="p2">(<span class="default">8 <a href="finseq_2.html#K4" title="FINSEQ_2:func.4">-tuples_on</a> <a href="margrel1.html#K5" title="MARGREL1:func.5">BOOLEAN</a></span>)</span></span>)</span>  ex <font color="Olive" title="b2">w</font> being    <a href="finseq_2.html#M2" title="FINSEQ_2:mode.2">Element</a> of <span class="p1">(<span class="default">4 <a href="xcmplx_0.html#K3" title="XCMPLX_0:func.3">*</a> <span class="p2">(<span class="default">7 <a href="xcmplx_0.html#K2" title="XCMPLX_0:func.2">+</a> <font color="Maroon" title="c2">m</font></span>)</span></span>)</span> <a href="finseq_2.html#K4" title="FINSEQ_2:func.4">-tuples_on</a> <span class="p1">(<span class="default">4 <a href="finseq_2.html#K4" title="FINSEQ_2:func.4">-tuples_on</a> <span class="p2">(<span class="default">8 <a href="finseq_2.html#K4" title="FINSEQ_2:func.4">-tuples_on</a> <a href="margrel1.html#K5" title="MARGREL1:func.5">BOOLEAN</a></span>)</span></span>)</span> st <br/>( <font color="Olive" title="b2">w</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <span class="p1">(<span class="default"><a href="aescip_1.html#K14" title="AESCIP_1:func.14">KeyExpansionX</a> (<font color="Maroon" title="c1">SBT</font>,<font color="Maroon" title="c2">m</font>)</span>)</span> <a href="aescip_1.html#K1" title="AESCIP_1:func.1">.</a> <font color="Olive" title="b1">Key</font> &amp; (  for <font color="Olive" title="b3">i</font> being   <a href="nat_1.html#NM1" title="NAT_1:NM.1">Nat</a>  st <font color="Olive" title="b3">i</font> <a href="xxreal_0.html#NR3" title="XXREAL_0:NR.3">&lt;</a> 7 <a href="xcmplx_0.html#K2" title="XCMPLX_0:func.2">+</a> <font color="Maroon" title="c2">m</font> holds <br/><span class="p1">(<span class="default"><font color="Maroon" title="c3">I</font> <a href="aescip_1.html#K1" title="AESCIP_1:func.1">.</a> <font color="Olive" title="b1">Key</font></span>)</span> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <span class="p1">(<span class="default"><font color="Olive" title="b3">i</font> <a href="nat_1.html#K1" title="NAT_1:func.1">+</a> 1</span>)</span> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <span class="p1"><a href="finseq_4.html#K7" title="FINSEQ_4:func.7">&lt;*</a><span class="default"><span class="p2">(<span class="default"><font color="Olive" title="b2">w</font> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <span class="p3">(<span class="default"><span class="p4">(<span class="default">4 <a href="xcmplx_0.html#K3" title="XCMPLX_0:func.3">*</a> <font color="Olive" title="b3">i</font></span>)</span> <a href="nat_1.html#K1" title="NAT_1:func.1">+</a> 1</span>)</span></span>)</span>,<span class="p2">(<span class="default"><font color="Olive" title="b2">w</font> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <span class="p3">(<span class="default"><span class="p4">(<span class="default">4 <a href="xcmplx_0.html#K3" title="XCMPLX_0:func.3">*</a> <font color="Olive" title="b3">i</font></span>)</span> <a href="nat_1.html#K1" title="NAT_1:func.1">+</a> 2</span>)</span></span>)</span>,<span class="p2">(<span class="default"><font color="Olive" title="b2">w</font> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <span class="p3">(<span class="default"><span class="p4">(<span class="default">4 <a href="xcmplx_0.html#K3" title="XCMPLX_0:func.3">*</a> <font color="Olive" title="b3">i</font></span>)</span> <a href="nat_1.html#K1" title="NAT_1:func.1">+</a> 3</span>)</span></span>)</span>,<span class="p2">(<span class="default"><font color="Olive" title="b2">w</font> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <span class="p3">(<span class="defaul