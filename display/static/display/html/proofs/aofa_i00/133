<span class="kw">let </span><font color="Maroon" title="c15">n</font>, <font color="Maroon" title="c16">m</font> be    <a href="subset_1.html#M1" title="SUBSET_1:mode.1">Element</a> of  <a href="numbers.html#NK1" title="NUMBERS:NK.1">NAT</a> ; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> ( <font color="Maroon" title="c15">n</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon" title="c11">s</font> <a href="aofa_i00.html#K21" title="AOFA_I00:func.21">.</a> <font color="Maroon" title="c7">x</font> &amp; <font color="Maroon" title="c16">m</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon" title="c11">s</font> <a href="aofa_i00.html#K21" title="AOFA_I00:func.21">.</a> <font color="Maroon" title="c8">y</font> &amp; ( <font color="Maroon" title="c11">s</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a> <span class="p1">(<span class="default"><a href="funct_2.html#K9" title="FUNCT_2:func.9">Funcs</a> (<font color="Maroon" title="c2">X</font>,<a href="numbers.html#K4" title="NUMBERS:func.4">INT</a>)</span>)</span> <a href="aofa_i00.html#K1" title="AOFA_I00:func.1">\</a> (<font color="Maroon" title="c3">b</font>,<a href="numbers.html#K5" title="NUMBERS:func.5">0</a>) implies <font color="Maroon" title="c16">m</font> <a href="xxreal_0.html#NR4" title="XXREAL_0:NR.4">&gt;</a>  <a href="numbers.html#K5" title="NUMBERS:func.5">0</a>  ) &amp; ( <font color="Maroon" title="c16">m</font> <a href="xxreal_0.html#NR4" title="XXREAL_0:NR.4">&gt;</a>  <a href="numbers.html#K5" title="NUMBERS:func.5">0</a>  implies <font color="Maroon" title="c11">s</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a> <span class="p1">(<span class="default"><a href="funct_2.html#K9" title="FUNCT_2:func.9">Funcs</a> (<font color="Maroon" title="c2">X</font>,<a hr