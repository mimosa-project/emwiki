<span class="kw">thus </span><a NAME="E6:36_1_1"/>
<font color="Maroon" title="c4">F</font> <a href="nat_1.html#K8" title="NAT_1:func.8">.</a> <span class="p1">(<span class="default"><font color="Maroon" title="c7">n</font> <a href="nat_1.html#K1" title="NAT_1:func.1">+</a> 1</span>)</span> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <span class="p1">(<span class="default"><font color="Maroon" title="c4">F</font> <a href="nat_1.html#K8" title="NAT_1:func.8">.</a> <font color="Maroon" title="c7">n</font></span>)</span> <a href="xboole_0.html#K2" title="XBOOLE_0:func.2">\/</a> <span class="p1"> { <span class="default"> <span class="p2">(<span class="default"><span class="p3">(<span class="default"><a href="pua2mss1.html#K2" title="PUA2MSS1:func.2">Den</a> (<font color="Olive" title="b1">o</font>,<font color="Maroon" title="c1">A</font>)</span>)</span> <a href="funct_1.html#K1" title="FUNCT_1:func.1">.</a> <font color="Olive" title="b2">p</font></span>)</span> where <font color="Olive" title="b1">o</font> is    <a href="subset_1.html#M2" title="SUBSET_1:mode.2">Element</a> of  <a href="finseq_1.html#K4" title="FINSEQ_1:func.4">dom</a>  the <a href="unialg_1.html#U1" title="UNIALG_1:sel.1">charact</a> of <font color="Maroon" title="c1">A</font>, <font color="Olive" title="b2">p</font> is    <a href="finseq_2.html#M2" title="FINSEQ_2:mode.2">Element</a> of  the <a href="struct_0.html#U1" title="STRUCT_0:sel.1">carrier</a> of <font color="Maroon" title="c1">A</font> <a href="finseq_2.html#K3" title="FINSEQ_2:func.3">*</a>  : ( <font color="Olive" title="b2">p</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a>  <a href="relset_1.html#K1" title="RELSET_1:func.1">dom</a> <span class="p1">(<span class="default"><a href="pua2mss1.html#K2" title="PUA2MSS1:func.2">Den</a> (<font color="Olive" title="b1">o</font>,<font color="Maroon" title="c1">A</font>)</span>)</span> &amp;  <a href="relset_1.html#K2" title="RELSET_1:func.2">rng</a> <font color="Olive" title="b2">p</font> <a href="tarski.html#R1" title="TARSKI:pred.1">c=</a> <font color="Maroon" title="c4">F</font> <a href="nat_1.html#K8" title="NAT_1:func.8">.</a> <font color="Maroon" title="c7">n</font> ) </span> } </span> 
