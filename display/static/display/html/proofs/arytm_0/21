<span class="kw">let </span><font color="Maroon" title="c2">x</font>, <font color="Maroon" title="c3">y</font>, <font color="Maroon" title="c4">z</font> be    <a href="subset_1.html#M1" title="SUBSET_1:mode.1">Element</a> of  <a href="numbers.html#K1" title="NUMBERS:func.1">REAL</a> ; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide">  <a href="arytm_0.html#K2" title="ARYTM_0:func.2">*</a> (<font color="Maroon" title="c2">x</font>,<span class="p1">(<span class="default"><a href="arytm_0.html#K1" title="ARYTM_0:func.1">+</a> (<font color="Maroon" title="c3">y</font>,<font color="Maroon" title="c4">z</font>)</span>)</span>) <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="arytm_0.html#K1" title="ARYTM_0:func.1">+</a> (<span class="p1">(<span class="default"><a href="arytm_0.html#K2" title="ARYTM_0:func.2">*</a> (<font color="Maroon" title="c2">x</font>,<font color="Maroon" title="c3">y</font>)</span>)</span>,<span class="p1">(<span class="default"><a href="arytm_0.html#K2" title="ARYTM_0:func.2">*</a> (<font color="Maroon" title="c2">x</font>,<font color="Maroon" title="c4">z</font>)</span>)</span>)</span><br/>

