      x in {a1,a2,a3} or x in {a4,a5,a6,a7,a8,a9} by XBOOLE_0:def 3; then
      x = a1 or x = a2 or x = a3 or x = a4 or x = a5 or x = a6 or x = a7 or
      x = a8 or x = a9 by ENUMSET1:def 1,def 4;
