 for <font color="Olive" title="b1">N</font> being   <a href="matrix_6.html#V1" title="MATRIX_6:attr.1">invertible</a>  <a href="matrix_1.html#NM2" title="MATRIX_1:NM.2">Matrix</a> of 3,<a href="vectsp_1.html#K2" title="VECTSP_1:func.2">F_Real</a>  st <font color="Olive" title="b1">N</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <span class="p1"><a href="finseq_1.html#K11" title="FINSEQ_1:func.11">&lt;*</a><span class="default"><span class="p2"><a href="finseq_1.html#K11" title="FINSEQ_1:func.11">&lt;*</a><span class="default"><font color="Maroon" title="c11">ia</font>,<a href="numbers.html#K5" title="NUMBERS:func.5">0</a>,<a href="numbers.html#K5" title="NUMBERS:func.5">0</a></span><a href="finseq_1.html#K11" title="FINSEQ_1:func.11">*&gt;</a></span>,<span class="p2"><a href="finseq_1.html#K11" title="FINSEQ_1:func.11">&lt;*</a><span class="default"><a href="numbers.html#K5" title="NUMBERS:func.5">0</a>,<font color="Maroon" title="c12">ib</font>,<a href="numbers.html#K5" title="NUMBERS:func.5">0</a></span><a href="finseq_1.html#K11" title="FINSEQ_1:func.11">*&gt;</a></span>,<span class="p2"><a href="finseq_1.html#K11" title="FINSEQ_1:func.11">&lt;*</a><span class="default"><a href="numbers.html#K5" title="NUMBERS:func.5">0</a>,<a href="numbers.html#K5" title="NUMBERS:func.5">0</a>,<font color="Maroon" title="c13">ic</font></span><a href="finseq_1.html#K11" title="FINSEQ_1:func.11">*&gt;</a></span></span><a href="finseq_1.html#K11" title="FINSEQ_1:func.11">*&gt;</a></span> holds <br/><span class="p1">(<span class="default"><a href="anproj_8.html#K4" title="ANPROJ_8:func.4">homography</a> <font color="Olive" title="b1">N</font></span>)</span> <a href="funct_2.html#K3" title="FUNCT_2:func.3">.</a> <font color="Maroon" title="c4">P</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="anproj_9.html#K8" title="ANPROJ_9:func.8">Dir111</a> 
