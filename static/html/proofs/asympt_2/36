<span class="kw">let </span><font color="Maroon" title="c12">n</font> be    <a href="subset_1.html#M1" title="SUBSET_1:mode.1">Element</a> of  <a href="numbers.html#NK1" title="NUMBERS:NK.1">NAT</a> ; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> ( <font color="Maroon" title="c12">n</font> <a href="xxreal_0.html#NR2" title="XXREAL_0:NR.2">&gt;=</a> <font color="Maroon" title="c7">N1</font> <a href="nat_1.html#K1" title="NAT_1:func.1">+</a> <font color="Maroon" title="c9">N2</font> implies ( <span class="p1">(<span class="default"><font color="Maroon" title="c2">x</font> <a href="series_1.html#K1" title="SERIES_1:func.1">+</a> <font color="Maroon" title="c3">y</font></span>)</span> <a href="nat_1.html#K8" title="NAT_1:func.8">.</a> <font color="Maroon" title="c12">n</font> <a href="xxreal_0.html#R1" title="XXREAL_0:pred.1">&lt;=</a> <span class="p1">(<span class="default"><font color="Maroon" title="c6">c1</font> <a href="xcmplx_0.html#K2" title="XCMPLX_0:func.2">+</a> <font color="Maroon" title="c8">c2</font></span>)</span> <a href="xcmplx_0.html#K3" title="XCMPLX_0:func.3">*</a> <span class="p1">(<span class="default"><span class="p2">(<span class="default"><a href="asympt_1.html#K3" title="ASYMPT_1:func.3">seq_n^</a> <font color="Maroon" title="c1">k</font></span>)</span> <a href="nat_1.html#K8" title="NAT_1:func.8">.</a> <font color="Maroon" title="c12">n</font></span>)</span> &amp; <span class="p1">(<span class="default"><font color="Maroon" title="c2">x</font> <a href="series_1.html#K1" title="SERIES_1:func.1">+</a> <font color="Maroon" title="c3">y</font></span>)</span> <a href="nat_1.html#K8" title="NAT_1:func.8">.</a> <font color="Maroon" title="c12">n</font> <a href="xxreal_0.html#NR2" title="XXREAL_0:NR.2">&gt;=</a>  <a href="numbers.html#K5" title="NUMBERS:func.5">0</a>  ) )</span><br/>

