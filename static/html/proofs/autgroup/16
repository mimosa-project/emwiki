<span class="kw">assume </span><span class="kw">that </span><br/><a NAME="E1:16_2"/><span class="lab"><font color="Green" title="E17">A4</font></span>: 
( <font color="Maroon" title="c4">q</font> <a href="hidden.html#R2" title="HIDDEN:pred.2">in</a>  <a href="relset_1.html#K1" title="RELSET_1:func.1">dom</a> <font color="Maroon" title="c3">f</font> &amp; <font color="Maroon" title="c5">q1</font> <a href="hidden.html#R2" title="HIDDEN:pred.2">in</a>  <a href="relset_1.html#K1" title="RELSET_1:func.1">dom</a> <font color="Maroon" title="c3">f</font> )
