 for <font color="Olive" title="b1">Y</font> being    <a href="hidden.html#M2" title="HIDDEN:mode.2">set</a>   st <font color="Olive" title="b1">Y</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a> <font color="Maroon" title="c4">X</font> holds <br/><font color="Olive" title="b1">Y</font> is   <a href="alg_1.html#NM2" title="ALG_1:NM.2">Equivalence_Relation</a> of <font color="Maroon" title="c1">M</font>
 <div><a class="txt" onmouseover="tooltip.show('hs2',this)" onmouseout="tooltip.hide()" onclick="hs2(this)" href="javascript:()" title="23_1_1_1_2_3"><span class="kw">proof </span></a><div class="add">

<span class="kw">let </span><font color="Maroon" title="c5">Y</font> be    <a href="hidden.html#M2" title="HIDDEN:mode.2">set</a> ; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> ( <font color="Maroon" title="c5">Y</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a> <font color="Maroon" title="c4">X</font> implies <font color="Maroon" title="c5">Y</font> is   <a href="alg_1.html#NM2" title="ALG_1:NM.2">Equivalence_Relation</a> of <font color="Maroon" title="c1">M</font> )</span><br/>

<span class="kw">assume </span><a NAME="E1:23_1_1_1_2_3"/>
<font color="Maroon" title="c5">Y</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a> <font color="Maroon" title="c4">X</font>
 ; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> <font color="Maroon" title="c5">Y</font> is   <a href="alg_1.html#NM2" title="ALG_1:NM.2">Equivalence_Relation</a> of <font color="Maroon" title="c1">M</font></span><br/>

<span class="kw">then </span><span class="kw">consider </span><font color="Maroon" title="c6">R</font> being   <a href="algstr_4.html#V1" title="ALGSTR_4:attr.1">compatible</a>  <a href="alg_1.html#NM2" title="ALG_1:NM.2">Equivalence_Relation</a> of <font color="Maroon" title="c1">M</font><span class="kw"> such that </span><br/><a NAME="E3:23_1_1_1_2_3"/><span class="lab"><font color="Green" title="E9">A6</font></span>: 
