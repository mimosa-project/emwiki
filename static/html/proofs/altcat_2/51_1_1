<span class="kw">thus </span><a NAME="E10:51_1_1"/>
(  the <a href="struct_0.html#U1" title="STRUCT_0:sel.1">carrier</a> of <font color="Maroon" title="c9">D</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <span class="p1"><a href="domain_1.html#K6" title="DOMAIN_1:func.6">{</a><span class="default"><font color="Maroon" title="c2">o</font></span><a href="domain_1.html#K6" title="DOMAIN_1:func.6">}</a></span> &amp;  the <a href="altcat_1.html#U1" title="ALTCAT_1:sel.1">Arrows</a> of <font color="Maroon" title="c9">D</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> (<font color="Maroon" title="c2">o</font>,<font color="Maroon" title="c2">o</font>) <a href="funcop_1.html#K18" title="FUNCOP_1:func.18">:-&gt;</a> <span class="p1"><a href="altcat_1.html#K1" title="ALTCAT_1:func.1">&lt;^</a><span class="default"><font color="Maroon" title="c2">o</font>,<font color="Maroon" title="c2">o</font></span><a href="altcat_1.html#K1" title="ALTCAT_1:func.1">^&gt;</a></span> &amp;  the <a href="altcat_1.html#U2" title="ALTCAT_1:sel.2">Comp</a> of <font color="Maroon" title="c9">D</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <span class="p1"><a href="domain_1.html#K4" title="DOMAIN_1:func.4">[</a><span class="default"><font color="Maroon" title="c2">o</font>,<font color="Maroon" title="c2">o</font>,<font color="Maroon" title="c2">o</font></span><a href="domain_1.html#K4" title="DOMAIN_1:func.4">]</a></span> <a href="funcop_1.html#K17" title="FUNCOP_1:func.17">.--&gt;</a> <span class="p1">(<span class="default"> the <a href="altcat_1.html#U2" title="ALTCAT_1:sel.2">Comp</a> of <font color="Maroon" title="c1">C</font> <a href="altcat_1.html#K4" title="ALTCAT_1:func.4">.</a> (<font color="Maroon" title="c2">o</font>,<font color="Maroon" title="c2">o</font>,<font color="Maroon" title="c2">o</font>)</span>)</span> )
