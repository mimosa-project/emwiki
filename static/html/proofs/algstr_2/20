<span class="kw">let </span><font color="Maroon" title="c2">a</font>, <font color="Maroon" title="c3">b</font> be   <a href="struct_0.html#NM1" title="STRUCT_0:NM.1">Element</a> of <font color="Maroon" title="c1">L</font>; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide">  ex <font color="Olive" title="b1">x</font> being   <a href="struct_0.html#NM1" title="STRUCT_0:NM.1">Element</a> of <font color="Maroon" title="c1">L</font> st <font color="Maroon" title="c2">a</font> <a href="algstr_0.html#K1" title="ALGSTR_0:func.1">+</a> <font color="Olive" title="b1">x</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon" title="c3">b</font></span><br/>



<span class="kw">consider </span><font color="Maroon" title="c4">y</font> being   <a href="struct_0.html#NM1" title="STRUCT_0:NM.1">Element</a> of <font color="Maroon" title="c1">L</font><span class="kw"> such that </span><br/><a NAME="E2:20_5"/><span class="lab"><font color="Green" title="E19">A13</font></span>: 
<font color="Maroon" title="c2">a</font> <a href="algstr_0.html#K1" title="ALGSTR_0:func.1">+</a> <font color="Maroon" title="c4">y</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="struct_0.html#K4" title="STRUCT_0:func.4">0.</a> <font color="Maroon" title="c1">L</font>
 <span class="kw">by</span> <span class="lab"><a class="txt" href="#E3:20"><span class="lab"><font color="Green" title="E11">A2</font></span></a></span>;<br/>
<span class="kw">take </span><font color="Maroon" title="c5">x</font> = <font color="Maroon" title="c4">y</font> <a href="algstr_0.html#K1" title="ALGSTR_0:func.1">+</a> <font color="Maroon" title="c3">b</font>; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> <font color="Maroon" title="c2">a</font> <a href="algstr_0.html#K1" title="ALGSTR_0:func.1">+</a> <font color="Maroon" title="c5">x</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon" title="c3">b</font></span><br/>

<span class="kw">thus </span><font color="Maroon" title="c2">a</font> <a href="algstr_0.html#K1" title="ALGSTR_0:func.1">+</a> <font color="Maroon" title="c5">x</font> = 
<span class="p1">(<span class="default"><a href="struct_0.html#K4" title="STRUCT_0:func.4">0.</a> <font color="Maroon" title="c1">L</font></span>)</span> <a href="algstr_0.html#K1" title="ALGSTR_0:func.1">+</a> <font color="Maroon" title="c3">b</font>
<span class="kw">by</span> <span class="lab"><a class="txt" href="#E4:20"><span class="lab"><font color="Green" title="E12">A3</font></span></a>, <a class="txt" href="#E2:20_5"><span class="lab"><font color="Green" title="E19">A13</font></span></a></span>
<br/>.= 
<font color="Maroon" title="c3">b</font>
<span class="kw">by</span> <span class="lab"><a class="txt" href="#E8:20"><span class="lab"><font color="Green" title="E16">A8</font></span></a></span>
; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> verum</span><br/>


</div><span class="kw">end;</span></div>
<span class="kw">hence </span><a NAME="E12:20"/>
<font color="Maroon" title="c1">L</font> is   <a href="algstr_2.html#NM4" title="ALGSTR_2:NM.4">doublesidedQuasi-Field</a>
