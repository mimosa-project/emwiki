 ex <font color="Olive">b<sub>1</sub></font> being   <a href="unialg_1.html#V1" title="UNIALG_1:attr.1">strict</a>   <a href="unialg_1.html#V2" title="UNIALG_1:attr.2">partial</a>   <a href="unialg_1.html#V3" title="UNIALG_1:attr.3">quasi_total</a>   <a href="unialg_1.html#V4" title="UNIALG_1:attr.4">non-empty</a>   <a href="unialg_1.html#L1" title="UNIALG_1:struct.1">UAStr</a>  st <br/>( <font color="Olive">b<sub>1</sub></font> is  <a href="aofa_000.html#V3" title="AOFA_000:attr.3">with_empty-instruction</a>  &amp; <font color="Olive">b<sub>1</sub></font> is  <a href="aofa_000.html#V4" title="AOFA_000:attr.4">with_catenation</a>  &amp; <font color="Olive">b<sub>1</sub></font> is  <a href="aofa_000.html#V5" title="AOFA_000:attr.5">with_if-instruction</a>  &amp; <font color="Olive">b<sub>1</sub></font> is  <a href="aofa_000.html#V6" title="AOFA_000:attr.6">with_while-instruction</a>  &amp; <font color="Olive">b<sub>1</sub></font> is  <a href="aofa_000.html#V8" title="AOFA_000:attr.8">unital</a>  &amp; <font color="Olive">b<sub>1</sub></font> is  <a href="aofa_000.html#V7" title="AOFA_000:attr.7">associative</a>  )
