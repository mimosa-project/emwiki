<a NAME="E2:153_1_1"/><span class="kw">then </span><span class="lab"><font color="Green" title="E91">A1</font></span>: 
<span class="p1"><a href="zfmisc_1.html#K2" title="ZFMISC_1:func.2">[:</a><span class="default"><font color="Maroon" title="c2">S</font>, the <a href="struct_0.html#U1" title="STRUCT_0:sel.1">carrier</a> of <font color="Maroon" title="c1">A</font></span><a href="zfmisc_1.html#K2" title="ZFMISC_1:func.2">:]</a></span> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a> <span class="p1"> { <span class="default"> <font color="Olive" title="b1">Z</font> where <font color="Olive" title="b1">Z</font> is   <a href="subset_1.html#NM2" title="SUBSET_1:NM.2">Subset</a> of <span class="p1"><a href="zfmisc_1.html#K2" title="ZFMISC_1:func.2">[:</a><span class="default"><font color="Maroon" title="c2">S</font>, the <a href="struct_0.html#U1" title="STRUCT_0:sel.1">carrier</a> of <font color="Maroon" title="c1">A</font></span><a href="zfmisc_1.html#K2" title="ZFMISC_1:func.2">:]</a></span> : <font color="Maroon">S<sub>1</sub></font>[<font color="Olive" title="b1">Z</font>] </span> } </span> 
 
;<br/>
<span class="kw">then </span><span class="kw">reconsider </span><font color="Maroon" title="c7">IT</font> =  <a href="setfam_1.html#K1" title="SETFAM_1:func.1">meet</a> <span class="p1"> { <span class="default"> <font color="Olive" title="b1">Z</font> where <font color="Olive" title="b1">Z</font> is   <a href="subset_1.html#NM2" title="SUBSET_1:NM.2">Subset</a> of <span class="p1"><a href="zfmisc_1.html#K2" title="ZFMISC_1:func.2">[:</a><span class="default"><font color="Maroon" title="c2">S</font>, the <a href="struct_0.html#U1" title="STRUCT_0:sel.1">carrier</a> of <font color="Maroon" title="c1">A</font></span><a href="zfmisc_1.html#K2" title="ZFMISC_1:func.2">:]</a></span> : <font color="Maroon">S<sub>1</sub></font>[<font color="Olive" title="b1">Z</font>] </span> } </span>  as   <a href="subset_1.html#NM2" title="SUBSET_1:NM.2">Subset</a> of <span class="p1"><a href="zfmisc_1.html#K2" title="ZFMISC_1:func.2">[:</a><span class="default"><font color="Maroon" title="c2">S</font>, the <a href="struct_0.html#U1" title="STRUCT_0:sel.1">carrier</a> of <font color="Maroon" title="c1">A</font></span><a href="zfmisc_1.html#K2" title="ZFMISC_1:func.2">:]</a></span> <span class="kw">by</span> <span class="lab"><a class="ref" href="setfam_1.html#T3" onmouseover="rs('setfam_1/T3')" onmouseout="rh()">SETFAM_1:3</a></span>;<br/>
<span class="kw">take </span>
