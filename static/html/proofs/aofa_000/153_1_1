<span class="kw">assume </span><a NAME="E9:153_1_1"/>
<font color="Maroon">S<sub>1</sub></font>[<font color="Maroon" title="c8">Y</font>]
 ; <a class="txt" onmouseover="tooltip.show('hs',this)" onmouseout="tooltip.hide()" onclick="hs(this)" href="javascript:()"><span class="comment"><font color="firebrick">::  thesis: </font></span></a><span class="hide"> <font color="Maroon" title="c7">IT</font> <a href="relset_1.html#R1" title="RELSET_1:pred.1">c=</a> <font color="Maroon" title="c8">Y</font></span><br/>

<a NAME="E10:153_1_1"/><span class="kw">then </span>
<font color="Maroon" title="c8">Y</font> <a href="tarski.html#R2" title="TARSKI:pred.2">in</a> <span class="p1"> { <span class="default"> <font color="Olive" title="b1">Z</font> where <font color="Olive" title="b1">Z</font> is   <a href="subset_1.html#NM2" title="SUBSET_1:NM.2">Subset</a> of <span class="p1"><a href="zfmisc_1.html#K2" title="ZFMISC_1:func.2">[:</a><span class="default"><font color="Maroon" title="c2">S</font>, the <a href="struct_0.html#U1" title="STRUCT_0:sel.1">carrier</a> of <font color="Maroon" title="c1">A</font></span><a href="zfmisc_1.html#K2" title="ZFMISC_1:func.2">:]</a></span> : <font color="Maroon">S<sub>1</sub></font>[<font color="Olive" title="b1">Z</font>] </span> } </span> 
 
;<br/>
<span class="kw">hence </span><a NAME="E11:153_1_1"/>
<font color="Maroon" title="c7">IT</font> <a href="relset_1.html#R1" title="RELSET_1:pred.1">c=</a> <font color="Maroon" title="c8">Y</font>
