<span class="kw">thus </span><a NAME="E8:244_1_1"/>
( (  for <font color="Olive" title="b1">c</font> being   <a href="abcmiz_1.html#V2" title="ABCMIZ_1:attr.2">constructor</a>  <a href="msualg_1.html#NM2" title="MSUALG_1:NM.2">OperSymbol</a> of <font color="Maroon" title="c1">C</font><br/>  for <font color="Olive" title="b2">p</font>, <font color="Olive" title="b3">q</font> being    <a href="trees_4.html#M1" title="TREES_4:mode.1">FinSequence</a> of  <a href="abcmiz_1.html#K34" title="ABCMIZ_1:func.34">QuasiTerms</a> <font color="Maroon" title="c1">C</font>  st  <a href="finseq_1.html#K3" title="FINSEQ_1:func.3">len</a> <font color="Olive" title="b2">p</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a>  <a href="finseq_1.html#K3" title="FINSEQ_1:func.3">len</a> <span class="p1">(<span class="default"><a href="msualg_1.html#K1" title="MSUALG_1:func.1">the_arity_of</a> <font color="Olive" title="b1">c</font></span>)</span> &amp; <font color="Olive" title="b3">q</font> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Maroon" title="c3">f9</font> <a href="lang1.html#K11" title="LANG1:func.11">*</a> <font color="Olive" title="b2">p</font> holds <br/><font color="Maroon" title="c3">f9</font> <a href="funct_2.html#K3" title="FUNCT_2:func.3">.</a> <span class="p1">(<span class="default"><font color="Olive" title="b1">c</font> <a href="abcmiz_1.html#K36" title="ABCMIZ_1:func.36">-trm</a> <font color="Olive" title="b2">p</font></span>)</span> <a href="hidden.html#R1" title="HIDDEN:pred.1">=</a> <font color="Olive" title="b1">c</font> <a href="abcmiz_1.html#K36" title="ABCMIZ_1:func.36">-trm</a> <font color="Olive" title="b3">q</font> ) &amp; (  for <font color="Olive" title="b1">a</font> being    <a href="abcmiz_1.html#M1" title="ABCMIZ_1:mode.1">expression</a> of <font color="Maroon" title="c1">C</font>, <a href="abcmiz_1.html#K13" title="ABCMIZ_1:func.13">an_Adj</a> <font color="Maroon" title="c1">C</font> holds  <font color="Maroon" title="c3">f9</font> <a href="funct_2.html#K3" title="FUNCT_2:func